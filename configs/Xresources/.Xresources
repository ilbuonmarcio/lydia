! ------------------------------------------------------------------------------
! rofi mrczlnks color theme
! ------------------------------------------------------------------------------
rofi.location:          0
rofi.color-enabled:     true
!                       'background', 'border'
rofi.color-window:      argb:ff22103a, argb:ffcccccc
! State:                'bg',          'fg',   'bgalt',       'hlbg',        'hlfg'
rofi.color-normal:      argb:00e9e9e9, #f9f9f9, argb:11751b7b, argb:77751b7b, argb:ffffffff
rofi.color-urgent:      argb:00553333, #999999, argb:00333333, argb:00888888, argb:ffffffff
rofi.separator-style:   solid
rofi.fullscreen:        false
rofi.padding:           18
rofi.hide-scrollbar:    true
rofi.line-margin:       2
rofi.line-padding:      2
rofi.fake-transparency: false
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             900
rofi.opacity:           30
rofi.font:              Roboto Regular 11
